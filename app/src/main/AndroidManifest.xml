<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.luna.androidlearning">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
            android:icon="@drawable/luna_logo"
        android:label="@string/app_name"
            android:roundIcon="@drawable/luna_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.luna.androidlearning.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.luna.androidlearning.LinearlayoutActivity" />
        <activity android:name="com.luna.androidlearning.RelativelayoutActivity" />
        <activity android:name="com.luna.androidlearning.TextViewActivity" />
        <activity android:name="com.luna.androidlearning.ButtonActivity" />
        <activity android:name="com.luna.androidlearning.EditTextActivity" />
        <activity android:name="com.luna.androidlearning.RatioButtonActivity" />
        <activity android:name="com.luna.androidlearning.CheckBoxActivity" />
        <activity android:name="com.luna.androidlearning.ImageViewActivity" />
        <activity android:name="com.luna.androidlearning.listview.ListViewActivity" />
        <activity android:name="com.luna.androidlearning.gridview.GridViewActivity" />
        <activity android:name="com.luna.androidlearning.ScrollViewActivity" />
        <activity android:name="com.luna.androidlearning.recyclerview.RecyclerViewActivity" />
        <activity android:name="com.luna.androidlearning.recyclerview.LinearRecyclerViewActivity" />
        <activity android:name="com.luna.androidlearning.recyclerview.HorRecyclerActivity" />
        <activity android:name="com.luna.androidlearning.recyclerview.GridRecyclerViewActivity" />
        <activity android:name="com.luna.androidlearning.recyclerview.PuRecyclerViewActivity" />
        <activity android:name="com.luna.androidlearning.WebViewActivity" />
        <activity android:name="com.luna.androidlearning.ToastActivity" />
        <activity android:name="com.luna.androidlearning.UIActivity" />
        <activity android:name="com.luna.androidlearning.DialogActivity" />
        <activity android:name="com.luna.androidlearning.ProgressActivity" />
        <activity android:name="com.luna.androidlearning.CustomDialogActivity" />
        <activity android:name="com.luna.androidlearning.PopupWindowActivity"></activity>
    </application>

</manifest>